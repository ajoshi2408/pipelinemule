#%RAML 1.0 DataType
type: object
properties:
  subject:
    description: Subject that needs to be sent to C-Sight for case creation
    type: string
  errorMessage:
    description: Description that needs to be sent C-Sight for case creation
    type: string
  emailTemplateConfigStoreKey:
    description: This values help in retrieving the notification template from database 
    type: string
