    required: false
  ownerId:
    description : OwnerId value related to Specific interface
    type: string
    required: false
  recordTypeId:
    description : RecordTypeId value related to Specific interface
    type: string
    required: false
  pricingType:
    description : PricingType value related to Specific interface
    type: string
    required: false
  issueType:
    description : IssueType value related to Specific interface
    type: string
    required: false
  resolutionCategory:
    description : ResolutionCategory value related to Specific interface
    type: string
    required: false